{
  "name": "canvaslens",
  "version": "1.0.0",
  "description": "A powerful HTML5 Canvas-based image viewing and annotation library",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "node server.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:silent": "jest --silent"
  },
  "keywords": [
    "canvas",
    "image-viewer",
    "annotation",
    "zoom",
    "pan",
    "image-comparison",
    "typescript",
    "html5-canvas",
    "before-after",
    "image-annotation"
  ],
  "author": "CanvasLens Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/koniz-dev/canvaslens.git"
  },
  "bugs": {
    "url": "https://github.com/koniz-dev/canvaslens/issues"
  },
  "homepage": "https://github.com/koniz-dev/canvaslens#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0"
  }
}
